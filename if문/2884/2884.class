import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);

        int H = scan.nextInt();
        int M = scan.nextInt();

        if (0 <= H && H <= 23 && 0 <= M && M <= 59) {
        if (M < 45 ) {
            M = 60 - (45 - M);
            if (H == 0) {
                H = 23;
            } else {
                H = H - 1;
            }
        } else {
            M = M - 45;
        }
    }
        System.out.println(H + " " + M);
    }
}
