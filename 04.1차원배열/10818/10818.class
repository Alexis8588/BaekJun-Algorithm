import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        int N = scan.nextInt();

        int[] num = new int[N];

        int max = 0;
        int min = 0;
        for (int i = 0; i < num.length; i++) {
            num[i] = scan.nextInt();
        }

        max = num[0];
        min = num[0];
        for (int i = 0; i < num.length; i++) {
            if (max < num[i]) {
                max = num[i];
            }

            if (min > num[i]) {
                min = num[i];
            }
        }
        System.out.println(min + " " + max);

    }
}
