

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int star = scan.nextInt();
        for (int i = 1; i <= star; i++) {
            for (int j = 1; j <=i+1; j++) {
                if (j <= i) {
                    System.out.print("*");
                } else {
                    System.out.println();
                }
            }
        }
    }
}